source 'https://github.com/CocoaPods/Specs.git'

#Uncomment the next line to define a global platform for your project
# platform :ios, ’10.0’

use_frameworks!

target 'AudioShare' do
	pod ‘Firebase/Core’
	pod ‘Firebase/Auth’
	pod ‘Firebase/Database’
	pod ‘Firebase/Storage’
	pod 'Alamofire', '~> 4.0’

end
